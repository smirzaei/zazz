<div class="container" style="margin-top: 50px;">
    <div>

        <div class="albim-pic-switch">
            <div class="btn-group" data-toggle="buttons-radio">
                <button class="btn btn-large btn-info active">Photos</button>
                <button class="btn btn-large btn-info">Albums</button>
            </div>
        </div>

        <div class="pic-list well">
            @for (int i = 0; i < 50; i++)
            {
                <div class="img">
                    <button class="btn btn-small remove-img icononly-btn" title="Remove"><i class="icon-remove"></i></button>
                    <img src="~/Images/placeholder.gif" class="img-polaroid" />
                </div>
            }
            <div class="clearfix"></div>
        </div>

    </div>
</div>
