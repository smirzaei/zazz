@{
    Layout = null;
}

<ul class="notification-list">
    @for (int i = 0; i < 5; i++)
    {
        <li>
            <img src="~/Images/placeholder.gif" alt="UserName" />
            <a href="#"><strong>UserName</strong></a>
            commented on
                <a href="#"><strong>This post</strong></a>
            <span class="font-verysmall pull-right time">1 hour ago</span>
        </li>
    }
</ul>
