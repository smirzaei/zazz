@model PhotoViewModel[]
@{
    Layout = null;
}

<div class="thumbnail-163">
    <a data-toggle="lightbox" data-target="#photo-@Model[0].PhotoId"
                                           href="@Model[0].PhotoUrl">
        <img src="@Model[0].PhotoUrl"/>
    </a>

</div>

<div class="thumbnail-163">
    <a data-toggle="lightbox" data-target="#photo-@Model[1].PhotoId"
                           href="@Model[1].PhotoUrl">
        <img src="@Model[1].PhotoUrl"/>
    </a>

</div>

<div class="thumbnail-163" style="margin-right: 0;">
    <a data-toggle="lightbox" data-target="#photo-@Model[2].PhotoId"
                                   href="@Model[2].PhotoUrl">
        <img src="@Model[2].PhotoUrl"/>
    </a>

</div>
